<div class="pull-right">
  <%= link_to 'People Page', people_path, class: 'btn btn-primary' %>
  <%= link_to 'Company Page', companies_path, class: 'btn btn-info' %>
</div>
<h2>Setup Page</h2>
<%= form_tag '/setup' do %>
    <%= text_area_tag :value, Setting.first.try(:value), rows: 5, cols: 100  %>
  <%= submit_tag 'Save', class: 'btn btn-default' %>
<% end %>